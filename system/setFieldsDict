FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue Z 0
);

regions
(
    zoneToCell
    {
        name "rotatingZone";
        fieldValues
        (
            volScalarFieldValue Z 1
        );
    }
);


